{
  "name": "cachesim",
  "version": "1.0.0",
  "description": "Cache Replacement Policy Simulator - Visualize and compare LRU, LFU, ARC, and FIFO policies",
  "main": "web/main.js",
  "scripts": {
    "build": "bash build.sh",
    "build:windows": "build.bat",
    "serve": "npx serve web",
